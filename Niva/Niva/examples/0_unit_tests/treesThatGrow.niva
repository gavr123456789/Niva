type Type name: String

union Statement =
| ^Expr
| Declaration

union Expr t: T =
| Red x: Int later: Expr::T
| Blue y: Int

type TypedExpr = Expr::Type
type UntypedExpr = Expr::Unit
type Later
Int x::Statement = [

  | x
  | Red => []
  | Blue => []
  | Declaration => []
]

