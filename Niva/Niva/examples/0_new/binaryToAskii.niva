String binaryToAscii = [
  result = this
    split: " ",
    map: [it toInt: 2, toChar],
    joinWith: ""

  ^ result]


{"01001000" "01101100 01101111" "01100101 01101100 01101100"} forEach: [
  it binaryToAscii
]
1..5 forEach: [
  it echo
]
"01001000 01100101 01101100 01101100 01101111" binaryToAscii echo


