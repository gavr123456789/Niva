enum CurrentOs = Win | Unix

String / x::String = [
  currentOs = CurrentOs.Unix
  slash =
    | currentOs
    | CurrentOs.Unix => "/"
    | CurrentOs.Win => "\\"
  ^ this + slash + x
]
