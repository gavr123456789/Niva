
UnaryMsg resolveUnaryFromDb2:  db::TyperDB withContext: c::Context -> UnaryMsgT = [
  kw = KeywordMsg
    args: {}
    receiver: receiver
    token: token
    kind: kind

  kwResolved = kw resolveKeywordFromDb2: db withContext: c nameOverride: selector name

  newUnary2 = UnaryMsgT
    selector: selector
    kind: kwResolved kind
    receiver: kwResolved receiver
    token: token
    typeInfo: kwResolved typeInfo
    dbMsg: kwResolved dbMsg

  ^ newUnary2
]
