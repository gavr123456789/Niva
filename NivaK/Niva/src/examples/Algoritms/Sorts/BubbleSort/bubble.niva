

MutableList::Int bubble = [
    array = this
    0 to: array count do: [ i ->
        0 to: array count - 2 - i do: [ j ->
            curr = array get: j
            next = array get: j + 1
            | curr > next => [
                temp = curr
                array at: j put: next
                array at: j + 1 put: temp
            ]
        ]
    ]
]

list = {3 2 1}
list bubble
//> [1, 2, 3]
> list
