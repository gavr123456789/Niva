{
    "configurations": [
        {
            "name": "Build",
            "type": "jps-build",
            "workingDir": "$PROJECT_DIR$",
            "module": "Niva",
            "includeTests": false
        },
        {
            "name": "MainKt",
            "type": "jps-run",
            "workingDir": "$PROJECT_DIR$",
            "dependsOn": [
                "Build"
            ],
            "mainClass": "MainKt",
            "module": "Niva",
            "options": [
                "-Dfile.encoding=UTF-8",
                "-classpath",
                "$PROJECT_DIR$/out/production/Niva:$USER_HOME$/.m2/repository/org/jetbrains/kotlin/kotlin-stdlib-jdk8/1.9.10/kotlin-stdlib-jdk8-1.9.10.jar:$USER_HOME$/.m2/repository/org/jetbrains/kotlin/kotlin-stdlib/1.9.10/kotlin-stdlib-1.9.10.jar:$USER_HOME$/.m2/repository/org/jetbrains/kotlin/kotlin-stdlib-common/1.9.10/kotlin-stdlib-common-1.9.10.jar:$USER_HOME$/.m2/repository/org/jetbrains/annotations/13.0/annotations-13.0.jar"
            ]
        }
        
    ]
}